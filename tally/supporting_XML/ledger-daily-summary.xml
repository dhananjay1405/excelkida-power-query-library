<?xml version="1.0" encoding="utf-8"?>
<ENVELOPE>
    <HEADER>
        <VERSION>1</VERSION>
        <TALLYREQUEST>Export</TALLYREQUEST>
        <TYPE>Data</TYPE>
        <ID>Ledger Daily</ID>
    </HEADER>
    <BODY>
        <DESC>
            <STATICVARIABLES>
                <SVEXPORTFORMAT>ASCII (Comma Delimited)</SVEXPORTFORMAT>
                <SVFROMDATE>$fromDate$</SVFROMDATE>
                <SVTODATE>$toDate$</SVTODATE>
                <LEDGERNAME>$ledgerName$</LEDGERNAME>
            </STATICVARIABLES>
        </DESC>
    </BODY>
</ENVELOPE>